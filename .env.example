# Environment Configuration for Weight Manager

# Server Configuration
PORT=3000
JWT_SECRET=your-secret-key-change-this-in-production

# Default Admin Credentials
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=admin123

# Ollama Configuration
OLLAMA_HOST=localhost
OLLAMA_PORT=11434
OLLAMA_MODEL=pranavverma/weight-manager:3b
OLLAMA_BASE_MODEL=llama3.2:3b
OLLAMA_TIMEOUT=30000

# Database Configuration
DB_PATH=./data/weight_manager.db

# Auto Nutrition Analysis
ENABLE_AUTO_NUTRITION=true
AUTO_NUTRITION_CONFIDENCE_THRESHOLD=0.7